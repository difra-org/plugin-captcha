{
    "name": "difra/plugin-captcha",
    "description": "Captcha plugin for Difra",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Ed Kutuev",
            "email": "ed@napych.com"
        }
    ],
    "require": {
        "difra/difra": "8.0.*",
        "ext-imagick": "*"
    },
    "dist": {
        "url": "https://github.com/difra-org/plugin-captcha/archive/refs/tags/8.0.0.zip",
        "type": "zip"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:difra-org/plugin-captcha.git"
        }
    ],
    "autoload": {
        "files": [
            "main.php"
        ],
        "classmap": [
            "src/Captcha.php"
        ],
        "psr-4": {
            "Difra\\Capcha\\": "src"
        }
    }
}
