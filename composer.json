{
    "name": "difra/capcha",
    "description": "Capcha plugin for Difra framework",
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Eduard Kutuev",
            "email": "nap@napych.com"
        }
    ],
    "require": {
        "difra/difra": "8.0.*"
    },
    "dist": {
        "url": "https://github.com/difra-org/capcha/archive/1.0.0.zip",
        "type": "zip"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:difra-org/capcha.git"
        }
    ],
    "autoload": {
        "files": [
            "main.php"
        ],
        "classmap": [
            "src/Capcha.php"
        ],
        "psr-4": {
            "Difra\\Capcha\\": "src"
        }
    }
}
